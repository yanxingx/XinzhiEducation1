<?xml version="1.0" encoding="UTF-8"?>
<web-app version="2.5" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
  <display-name></display-name>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginServlet</servlet-name>
    <servlet-class>servlet.LoginServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>RegisterServlet</servlet-name>
    <servlet-class>servlet.RegisterServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>GetAllStudentList</servlet-name>
    <servlet-class>servlet.GetAllStudentList</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>TeacherClaimStudentServlet</servlet-name>
    <servlet-class>servlet.TeacherClaimStudentServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>LoginByTokenServlet</servlet-name>
    <servlet-class>servlet.LoginByTokenServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>GetAllMyStudentList</servlet-name>
    <servlet-class>servlet.GetAllMyStudentList</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>TeacherUpdateStuHeartCoinServlet</servlet-name>
    <servlet-class>servlet.TeacherUpdateStuHeartCoinServlet</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>GetAllStudentWithoutToken</servlet-name>
    <servlet-class>servlet.GetAllStudentWithoutToken</servlet-class>
  </servlet>








  <servlet-mapping>
    <servlet-name>LoginServlet</servlet-name>
    <url-pattern>/servlet/LoginServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>RegisterServlet</servlet-name>
    <url-pattern>/servlet/RegisterServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetAllStudentList</servlet-name>
    <url-pattern>/servlet/GetAllStudentList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TeacherClaimStudentServlet</servlet-name>
    <url-pattern>/servlet/TeacherClaimStudentServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>LoginByTokenServlet</servlet-name>
    <url-pattern>/servlet/LoginByTokenServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetAllMyStudentList</servlet-name>
    <url-pattern>/servlet/GetAllMyStudentList</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>TeacherUpdateStuHeartCoinServlet</servlet-name>
    <url-pattern>/servlet/TeacherUpdateStuHeartCoinServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>GetAllStudentWithoutToken</servlet-name>
    <url-pattern>/servlet/GetAllStudentWithoutToken</url-pattern>
  </servlet-mapping>	
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
</web-app>
